';\nimport styled, { keyframes } from 'styled-components';\nimport { shade } from 'polished';\n\nexport const appearFromMiddle = keyframes`\n  from {\n    opacity: 0;\n    transform:translateY(-100%)\n  } to {\n    opacity: 1;\n    transform: translateY(0%)\n  }\n`;\n\nexport const LightBox = styled.div`\n  position: fixed; /* Stay in place */\n  z-index: 20; /* Sit on top */\n  left: 0;\n  top: 0;\n  width: 100%; /* Full width */\n  height: 100%; /* Full height */\n  overflow: auto; /* Enable scroll if needed */\n  background-color: rgba(0, 0, 0, 0.6); /* Black w/ opacity */\n  padding: 60px 20px;\n`;\n\nexport const ModalWrapper = styled.div`\n  position: relative;\n  margin: 0 auto;\n  border-radius: 10px;\n  width: 600px;\n  background-color: white;\n  box-shadow: 0 5px 16px rgba(0, 0, 0, 0.4);\n  animation: ${appearFromMiddle} 0.3s;\n\n  @media (max-width: 720px) {\n    width: 100%;\n  }\n`;\n\nexport const CloseModalButton = styled(MdClose)`\n  cursor: pointer;\n  position: absolute;\n  top: 20px;\n  right: 20px;\n  z-index: 10;\n  padding: 0;\n  font-size: 30px;\n  transition: color 0.2s;\n\n  &:hover {\n    opacity: 0.5;\n  }\n`;\n\nexport const Header = styled.div`\n  width: 100%;\n  border-bottom: 2px solid #eeee;\n  padding: 30px 20px;\n\n  h1 {\n    font-weight: 300;\n    font-size: 24px;\n  }\n`;\n","/home/wellington/Study/Projects/projeto-kripton-sports-web/src/components/SideNavBar/Data.tsx",[],"/home/wellington/Study/Projects/projeto-kripton-sports-web/src/components/Header/HeaderMenuDropdown/index.tsx",[],"/home/wellington/Study/Projects/projeto-kripton-sports-web/src/components/SideNavBar/NavLink/index.tsx",[],"/home/wellington/Study/Projects/projeto-kripton-sports-web/src/pages/SignIn/Input/Tootip/index.tsx",[],"/home/wellington/Study/Projects/projeto-kripton-sports-web/src/components/Header/HeaderMenuDropdown/style.ts",[],"/home/wellington/Study/Projects/projeto-kripton-sports-web/src/components/SideNavBar/NavLink/style.ts",[],"/home/wellington/S