e({\n        login: Yup.string()\n          .required('Campo obrigatório')\n          .min(4, 'minino 4 digitos'),\n        password: Yup.string()\n          .required('Campo obrigatório')\n          .min(8, 'minimo 8 digitos'),\n      });\n\n      const data = {\n        login,\n        password,\n      };\n\n      await schema.validate(data, {\n        abortEarly: false,\n      });\n\n      console.log(data);\n\n      setErrors({});\n    } catch (err) {\n      const getErrors = getValidationErrors(err);\n\n      setErrors(getErrors);\n    }\n  };\n\n  return (\n    <Container>\n      <Content>\n        <img src={LogoImg} alt=\"kripton suplementos\" />\n\n        <form onSubmit={handleSubmit}>\n          <div>\n            Login\n            <Input\n              name=\"login\"\n              type=\"text\"\n              icon={FiUser}\n              placeholder=\"Digite o login\"\n              value={login}\n              onChange={(e) => setLogin(e.target.value)}\n              error={errors.login}\n            />\n          </div>\n\n          <div>\n            Senha\n            <Input\n              name=\"password\"\n              type=\"password\"\n              icon={FiLock}\n              placeholder=\"Digite sua senha\"\n              value={password}\n              onChange={(e) => setPassword(e.target.value)}\n              error={errors.password}\n            />\n          </div>\n\n          <button type=\"submit\" value=\"submit\">\n            Entrar\n          </button>\n        </form>\n\n        <p>\n          Esqueceu a senha?\n          <a href=\"/\">Redefinir senha</a>\n        </p>\n      </Content>\n    </Container>\n  );\n};\n\nexport default SignIn;\n","/home/wellington/Study/Projects/projeto-kripton-sports-web/src/pages/Dashboard/index.tsx",[],"/home/wellington/Study/Projects/projeto-kripton-sports-web/src/pages/Collaborators/index.tsx",["151"],"import React, { useCallback, useState } from 'react';\n\nimport { FiUser } from 'react-icons/